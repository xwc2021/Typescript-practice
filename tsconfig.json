{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "module": "es6",
        "target": "es5",
        // "jsx": "react",
        "allowJs": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
    }
}